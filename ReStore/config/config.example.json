{
  "watchDirectories": [
    {
      "path": "%APPDATA%",
      "storageType": "gdrive"
    },
    {
      "path": "%USERPROFILE%\\Desktop",
      "storageType": null
    },
    {
      "path": "%USERPROFILE%\\Documents",
      "storageType": "local"
    },
    {
      "path": "%USERPROFILE%\\Pictures",
      "storageType": "s3"
    }
  ],
  "globalStorageType": "local",
  "backupInterval": "01:00:00",
  "sizeThresholdMB": 500,
  "maxFileSizeMB": 100,
  "backupType": "Incremental",
  "retention": {
    "enabled": false,
    "keepLastPerDirectory": 5,
    "maxAgeDays": 30
  },
  "systemBackup": {
    "enabled": true,
    "includePrograms": true,
    "includeEnvironmentVariables": true,
    "backupInterval": "1.00:00:00",
    "storageType": null,
    "programsStorageType": "local",
    "environmentStorageType": "gdrive",
    "settingsStorageType": "s3",
    "excludeSystemPrograms": [
      "Microsoft Visual C++",
      "Microsoft .NET",
      "Windows SDK",
      "KB[0-9]+",
      "Update for",
      "Security Update"
    ]
  },
  "excludedPatterns": [
    "*.tmp",
    "*.temp",
    "~$*",
    "Thumbs.db",
    ".DS_Store",
    "*.log",
    "*.pst",
    "desktop.ini",
    "*.lnk",
    "*.crdownload",
    "*.partial"
  ],
  "encryption": {
    "enabled": false,
    "salt": null,
    "keyDerivationIterations": 100000,
    "verificationToken": null
  },
  "excludedPaths": [
    "%USERPROFILE%\\Documents\\Temp",
    "%USERPROFILE%\\Downloads",
    "%TEMP%",
    "%LOCALAPPDATA%\\Temp",
    "%WINDIR%",
    "%ProgramFiles%",
    "%ProgramFiles(x86)%"
  ],
  "storageSources": {
    "gdrive": {
      "path": "./backups",
      "options": {
        "client_id": "",
        "client_secret": "",
        "token_folder": "",
        "backup_folder_name": "ReStoreBackups"
      }
    },
    "s3": {
      "path": "./backups",
      "options": {
        "accessKeyId": "",
        "secretAccessKey": "",
        "region": "",
        "bucketName": ""
      }
    },
    "github": {
      "path": "./backups",
      "options": {
        "token": "",
        "repo": "",
        "owner": ""
      }
    },
    "local": {
      "path": "%USERPROFILE%\\ReStoreBackups",
      "options": {}
    },
    "azure": {
      "path": "my-container",
      "options": {
        "connectionString": "",
        "containerName": "my-container"
      }
    },
    "gcp": {
      "path": "my-bucket",
      "options": {
        "bucketName": "my-bucket",
        "credentialPath": "C:\\path\\to\\key.json"
      }
    },
    "dropbox": {
      "path": "/Backups",
      "options": {
        "accessToken": "your-access-token",
        "refreshToken": "optional-refresh-token",
        "appKey": "optional-app-key",
        "appSecret": "optional-app-secret"
      }
    },
    "sftp": {
      "path": "/home/user/backups",
      "options": {
        "host": "sftp.example.com",
        "port": "22",
        "username": "user",
        "password": "",
        "privateKeyPath": "C:\\path\\to\\private_key",
        "passphrase": "optional-passphrase"
      }
    },
    "b2": {
      "path": "my-bucket",
      "options": {
        "keyId": "your-key-id",
        "applicationKey": "your-app-key",
        "serviceUrl": "https://s3.us-west-000.backblazeb2.com",
        "bucketName": "my-bucket"
      }
    }
  }
}